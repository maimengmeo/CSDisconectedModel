<Window x:Class="Week9DisconnectedModel.DataSetWithMulTables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week9DisconnectedModel"
        mc:Ignorable="d"
        Title="DataSetWithMulTables" Height="450" Width="800">
    <Canvas>
        <Button x:Name="btnLoad" Content="Load" Canvas.Left="25" Canvas.Top="62" FontSize="20" FontWeight="Bold" Height="47" Width="91" Click="btnLoad_Click"/>
        <DataGrid x:Name="grdCategories" Height="118" Width="375" d:ItemsSource="{d:SampleData ItemCount=5}" Canvas.Left="25" Canvas.Top="139"/>
        <DataGrid x:Name="grdProducts" Height="118" Width="375" d:ItemsSource="{d:SampleData ItemCount=5}" Canvas.Left="405" Canvas.Top="139" HorizontalAlignment="Left" VerticalAlignment="Center"/>

    </Canvas>
</Window>
